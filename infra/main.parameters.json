{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "resourcePrefix": {
      "value": "copilot-plugin"
    },
    "functionAppSku": {
      "value": "Y1"
    },
    "pythonVersion": {
      "value": "3.11"
    },
    "enableApplicationInsights": {
      "value": true
    },
    "enableKeyVault": {
      "value": true
    }
  }
}
